//номер 11 :



using System;
using System.Collections.Generetic;
using System.Linq;

class Program
{
    static void Main()
    {
        List<int> numbers = new List<int>();
        {  numbers.Add(1); }
        var result = GetNumbersAboveAverage(numbers);
        Console.WriteLine("Числа больше среднего значения");
        foreach (var number in result)
        {
            Console.WriteLine(number);
        }
        {
            static IEnumerable<int> GetNumbersAboveAverage(List<int> numbers);
            {
                double average = numbers.Average();
                return numbers.Where(n => n > average);
            }

        }
        
     
    }
}
